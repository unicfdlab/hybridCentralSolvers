FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    outlet
    {
        type            totalPressure;
        value		uniform 1e5;
        p0		uniform 1e5;
        phi		phi;
        rho		none;
        psi		thermo:psi;
        gamma		1;
    }

    inlet1
    {
        type            zeroGradient;
    }

    inlet2
    {
        type            zeroGradient;
    }

    walls
    {
        type            slip;
    }


    frontAndBack
    {
        type            empty;
    }
}

// ************************************************************************* //

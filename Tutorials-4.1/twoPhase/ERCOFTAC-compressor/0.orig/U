FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    INLET
    {
        type           surfaceNormalFixedValue;
        refValue       uniform -10.0;
        //type          flowRateInletVelocity;
        //massFlowRate  constant 0.7435e-3;
        //value         uniform (0 0 0);
    }

    BLADE_ROT
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);

    }
    HUB_SHROUD
    {
        type            empty;
    }
    AMI_INT
    {
        type            cyclicAMI;
        value           uniform (0 0 0);
    }
    AMI_EXT
    {
        type            cyclicAMI;
        value           uniform (0 0 0);
    }
    OUTLET
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    BLADE_STAT
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


